!!! 5
%html(lang='en')
  %head
    %meta(charset='utf-8')
    %meta(name='viewport' content='width=device-width,initial-scale=1')

    %link(href='/css/img/favicon.ico' rel='shortcut icon' type='image/x-icon')
    %link(rel='stylesheet' href='/css/vendor/reset.css')
    %link(rel='stylesheet' href='/css/vendor/jquery-ui.min.css')
    %link(rel='stylesheet' href='/css/style.css')

    -# Order matters, do not reorder without good reason.
    %script(src='/js/lib/vendor/jquery.min.js')
    %script(src='/js/lib/vendor/jquery-ui.min.js')
    %script(src='/js/lib/vendor/jquery.form.js')
    %script(src='/js/lib/vendor/URI.js')
    %script(src='/js/app.js')
    %script(src='/js/osmtags.js')

    %title CitySDK CMS

  %body
    .full-width-header
      .container
        %header.header
    .full-width-content
      #maincontent.container
        = yield
    .full-width-footer
      .container
        %footer.footer.clearfix
          %img.logo(src='/css/img/logos.jpg')

